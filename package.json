      "before",
      "beforeEach",
      "describe",
      "it"
    ]
  },
  "dependencies": {
    "@google-cloud/datastore": "^0.1.1",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "helmet": "^3.5.0",
    "kerberos": "^0.0.21",
    "lodash": "^4.14.2",
    "mongodb": "^2.2.5",
    "mysql": "^2.11.1",
    "nconf": "^0.8.4",
    "nunjucks": "^3.0.0",
    "nunjucks-date-filter": "^0.1.1",
    "prompt": "^1.0.0"
  },
  "devDependencies": {
    "mocha": "^3.0.2",
    "nodejs-repo-tools": "git+https://git@github.com/GoogleCloudPlatform/nodejs-repo-tools.git",
    "proxyquire": "^1.7.10",
    "sinon": "^1.17.5"
  },
  "engines": {
    "node": ">=4.3.2"
  }
